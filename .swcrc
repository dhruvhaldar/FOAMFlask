{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": false,
            "dynamicImport": true
        },
        "target": "es2020",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": true
    },
    "module": {
        "type": "es6"
    },
    "sourceMaps": true
}